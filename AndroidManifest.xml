<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.write.Quill" android:versionCode="5" android:versionName="3.2">
    <uses-sdk android:minSdkVersion="12" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application 
        android:label="@string/app_name" 
        android:hardwareAccelerated="true" 
        android:theme="@android:style/Theme.Holo.Light" 
        android:icon="@drawable/icon">
        <activity android:label="@string/app_name" android:name="QuillWriterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.write.Quill.Preferences"></activity>
        <activity android:name="com.write.Quill.ExportActivity"
            android:theme="@android:style/Theme.Holo.Light.Dialog" 
        ></activity>

    </application>
</manifest>